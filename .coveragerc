[run]
# Coverage.py configuration

# Source packages
source = 
    services
    customer_agents
    shared

# Omit files
omit =
    */tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */virtualenv/*
    */.venv/*
    */node_modules/*
    */migrations/*
    */config/*
    */setup.py
    */conftest.py
    */__init__.py

# Parallel execution
parallel = True
concurrency = multiprocessing

# Context
context = ${CONTEXT}

[report]
# Reporting
precision = 2
show_missing = True
skip_covered = False
skip_empty = True

# Exclude patterns
exclude_lines =
    # Standard pragma
    pragma: no cover
    
    # Debug code
    def __repr__
    if self\.debug
    if settings\.DEBUG
    
    # Defensive programming
    raise AssertionError
    raise NotImplementedError
    
    # Non-runnable
    if 0:
    if False:
    if __name__ == .__main__.:
    
    # Abstract methods
    @(abc\.)?abstractmethod
    
    # Type checking
    if TYPE_CHECKING:
    @overload
    
    # Logging
    logger\.debug
    logging\.debug

# Ignore errors
ignore_errors = False

[html]
# HTML report
directory = htmlcov
title = AutoGen Platform Coverage Report
show_contexts = True

[xml]
# XML report for CI
output = coverage.xml

[json]
# JSON report
output = coverage.json
pretty_print = True
show_contexts = True